{
	"servers": {
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"C:/Users/Neff_PM/Documents/ChambitasUwU"
			],
			"type": "stdio"
		},
		"mcp_server_mysql": {
			"command": "node",
			"args": [
				"C:/Users/Neff_PM/Documents/mcp-server-mysql/dist/index.js"
			],
			"env": {
				"MYSQL_HOST": "127.0.0.1",
				"MYSQL_PORT": "3306",
				"MYSQL_USER": "root",
				"MYSQL_PASS": "1234",
				"MYSQL_DB": "ReGpsBase",
				"ALLOW_INSERT_OPERATION": "false",
				"ALLOW_UPDATE_OPERATION": "false",
				"ALLOW_DELETE_OPERATION": "false"
			},
			"type": "stdio"
		},
		"github": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		},
		"deepwiki": {
			"url": "https://mcp.deepwiki.com/sse",
			"type": "http"
		},
		"vscode-mcp-server": {
			"command": "npx",
			"args": [
				"mcp-remote@next",
				"http://localhost:3000/mcp"
			],
			"type": "stdio"
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		"memory": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory"
			],
			"env": {
				"MEMORY_FILE_PATH": "${input:memory_file_path}"
			},
			"type": "stdio"
		},
		"figma-developer-mcp": {
			"command": "npx",
			"args": [
				"-y",
				"figma-developer-mcp",
				"--stdio"
			],
			"env": {
				"FIGMA_API_KEY": "${input:figmaApiKey}"
			},
			"type": "stdio"
		},
		"Framelink Figma MCP": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"figma-developer-mcp",
				"--figma-api-key=YOUR-KEY",
				"--stdio"
			],
			"type": "stdio"
		},
		"mysql": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"--env-file",
				"C:/mpcmysql/mysql.env",
				"--from",
				"mysql-mcp-server",
				"mysql_mcp_server"
			],
			"env": {}
		},
		"context7": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"type": "stdio"
		},
		"postmanlabs/postman-mcp-server": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@postman/postman-mcp-server@latest"
			],
			"env": {
				"POSTMAN_API_KEY": "${input:api_key}"
			},
			"gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/40516270-789a-bcde-f012-345678901234",
			"version": "1.0.0"
		},
		"com.gitlab/mcp": {
			"url": "https://gitlab.com/api/v4/mcp",
			"type": "http"
		}
	},
	"inputs": [
		{
			"id": "memory_file_path",
			"type": "promptString",
			"description": "Path to the memory storage file (optional)",
			"password": false
		},
		{
			"type": "promptString",
			"id": "FIGMA_API_KEY",
			"description": "Your Figma Personal Access Token, learn more here: https://www.figma.com/developers/api#access-tokens",
			"password": true
		},
		{
			"type": "promptString",
			"id": "figmaApiKey",
			"description": "Figma Personal Access Token",
			"password": true
		},
		{
			"type": "promptString",
			"id": "allowedDir",
			"description": "Path to allowed directory to expose to the MCP server (container path). For multiple directories, separate with spaces.",
			"password": false
		},
		{
			"id": "api_key",
			"type": "promptString",
			"description": "Your API key for the service",
			"password": true
		}
	]
}